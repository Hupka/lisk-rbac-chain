{
	"name": "lisk-rbac-chain",
	"version": "0.1.0",
	"description": "Demo application for the standalone module &#39;Lisk RBAC&#39;",
	"author": "hupka.adrian@gmail.com",
	"license": "MIT",
	"keywords": [
		"blockchain",
		"lisk",
		"nodejs",
		"javascript",
		"typescript"
	],
	"homepage": "./src/app/plugins/lisk_rbac_ui",
	"repository": {},
	"engines": {
		"node": ">=12.13.0 <=12",
		"npm": ">=6.12.0"
	},
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"format": "prettier --write '**/*'",
		"prepack": "oclif-dev manifest && oclif-dev readme --multi --dir=docs/commands && npm shrinkwrap && npm prune --production && npm shrinkwrap",
		"prebuild": "if test -d dist; then rm -r dist; fi; rm -f tsconfig.tsbuildinfo; rm -f npm-shrinkwrap.json",
		"start": "echo Run \"./bin/run start\" to start the app",
		"build": "tsc",
		"start:ui": "npm run watch:css && react-scripts start",
		"build:ui": "npm run watch:css && react-scripts build",
		"test": "jest --passWithNoTests",
		"test:coverage": "jest --passWithNoTests --coverage=true --coverage-reporters=text",
		"test:ci": "jest --passWithNoTests --coverage=true --coverage-reporters=json --verbose",
		"version": "oclif-dev readme --multi --dir=docs/commands && git add README.md docs",
		"prepublishOnly": "npm ci && npm run lint && npm run build",
		"watch:css": "postcss src/ui/styles/tailwind.css -o src/ui/styles/output.css"
	},
	"bin": {
		"lisk-rbac-chain": "./bin/run"
	},
	"lisk": {
		"addressPrefix": "lsk"
	},
	"oclif": {
		"bin": "lisk-rbac-chain",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-help"
		]
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/dist",
		"/config",
		"/docs"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@headlessui/react": "^1.3.0",
		"@heroicons/react": "^1.0.1",
		"@liskhq/lisk-framework-dashboard-plugin": "^0.1.0",
		"@liskhq/lisk-framework-faucet-plugin": "^0.1.0",
		"@oclif/command": "1.8.0",
		"@oclif/plugin-autocomplete": "0.3.0",
		"@oclif/plugin-help": "3.2.2",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"@types/jest": "^26.0.23",
		"@types/node": "^12.20.15",
		"@types/react": "^17.0.13",
		"@types/react-dom": "^17.0.8",
		"axios": "0.21.1",
		"fs-extra": "9.0.1",
		"inquirer": "7.3.2",
		"lisk-commander": "^5.1.0",
		"lisk-framework": "^0.7.6",
		"lisk-rbac": "^1.1.1",
		"lisk-sdk": "^5.1.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"tailwindcss": "^2.2.4",
		"tar": "6.0.2",
		"tslib": "1.13.0",
		"web-vitals": "^1.1.2"
	},
	"devDependencies": {
		"@oclif/dev-cli": "1.22.2",
		"@oclif/config": "1.15.1",
		"@types/fs-extra": "9.0.1",
		"@types/node": "12.20.6",
		"@types/tar": "4.0.3",
		"@types/jest": "26.0.21",
		"@types/jest-when": "2.7.2",
		"@typescript-eslint/eslint-plugin": "4.19.0",
		"@typescript-eslint/parser": "4.19.0",
		"eslint": "7.22.0",
		"eslint-config-lisk-base": "2.0.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jest": "24.3.2",
		"globby": "10.0.2",
		"husky": "4.2.5",
		"jest": "26.6.0",
		"jest-extended": "0.11.5",
		"jest-when": "3.2.1",
		"lint-staged": "10.2.11",
		"prettier": "2.2.1",
		"ts-jest": "26.5.4",
		"ts-node": "9.1.1",
		"typescript": "4.2.3",
		"autoprefixer": "^10.2.6",
		"eslint-config-prettier": "^8.1.0",
		"postcss": "^8.3.5",
		"postcss-cli": "^8.3.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
